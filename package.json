{
  "name": "@nxus/users",
  "version": "2.4.0-0",
  "description": "User management module for Nxus apps.",
  "main": "lib/index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --compilers js:babel/register -R spec",
    "compile": "babel --optional runtime -d lib/ src/",
    "prepublish": "npm run compile",
    "postpublish": "npm run docs-readme && npm run commit-update-docs",
    "docs-readme": "export NAME=`npm view . name`; export VERSION=`npm view . version`; documentation readme ./src/*.js --name $NAME --project-version $VERSION --readme-file ./README.md -s $NAME",
    "commit-update-docs": "git add ./README.md && git commit -m 'Updated README API Docs' && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nxus/users.git"
  },
  "keywords": [
    "nxus",
    "user management",
    "users",
    "authentication"
  ],
  "author": "mjreich",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nxus/users/issues"
  },
  "peerDependencies": {
    "@nxus/core": "^2.4.0-0",
    "@nxus/storage": "^2.4.0-0",
    "@nxus/mailer": "^2.4.0-0"
  },
  "devDependencies": {
    "@nxus/core": "^2.4.0-0",
    "babel": "^5.6.23",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "mocha": "^2.2.5",
    "sails-memory": "^0.10.5",
    "sinon": "^1.17.2",
    "@nxus/storage": "^2.4.0-0",
    "documentation": "^4.0.0-beta"
  },
  "dependencies": {
    "babel-runtime": "^5.7.0",
    "connect-waterline": "^0.1.0",
    "cookie-session": "^1.1.0",
    "cryptojs": "^2.5.3",
    "express-session": "^1.12.1",
    "glob": "^6.0.1",
    "passport": "^0.2.1",
    "passport-local": "^1.0.0",
    "routes": "^2.1.0",
    "session-file-store": "0.0.24",
    "underscore": "^1.7.0"
  },
  "homepage": "https://github.com/nxus/users#readme"
}
